{% extends '@MainApp/base/base.html.twig' %}
{% set page_title = 'Интеграция с ВИС Московской области' %}
{% set sub_page_title = 'Настройка модуля интеграции' %}

{% block title %}
    {% if page_title is defined %} {{ page_title }} {% endif %}
{% endblock %}
{% set topmenu = true %}
{% set actions_menu = true %}


{% block topmenu_items %}
    <button type="submit" form="mod_mosregvis_editform"
       class="btn btn-light btn-sm w-100 js-bs-popover-enabled"
       data-bs-toggle="popover"
       data-bs-placement="top" title=""
       data-bs-content="Добавить"
       data-bs-original-title="Добавить новую группу">
        <i class="far fa-fw fa-save"></i> Сохранить
    </button>
{% endblock %}

{% block body %}
    {{ dump() }}
    <div {{ stimulus_controller('mod-mosreg-vis--mod-mosregvis-edit') }}>
        {{ include('@mod_mosregvis/$modMosregVis_Configuration/_edit_form.html.twig') }}

        {{ include('@mod_mosregvis/$modMosregVis_Configuration/_delete_form.html.twig') }}
    </div>
{% endblock %}
