{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/doctrine-orm": "^4.0",
        "api-platform/graphql": "^4.0",
        "api-platform/symfony": "^4.0",
        "doctrine/annotations": "^2.0",
        "doctrine/dbal": "3.9.*",
        "doctrine/doctrine-bundle": "2.*",
        "doctrine/doctrine-migrations-bundle": "3.*",
        "doctrine/event-manager": "^2",
        "doctrine/orm": "^3",
        "doctrine/persistence": "^3.1|^4",
        "dompdf/dompdf": "*",
        "gesdinet/jwt-refresh-token-bundle": "^1.4",
        "heymoon/doctrine-psql-enum": "^2.1",
        "knplabs/knp-menu-bundle": "^3.2",
        "lexik/jwt-authentication-bundle": "^3.1",
        "nelmio/cors-bundle": "^2.5",
        "npub/gos": "^v0.9.13",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpoffice/phpspreadsheet": "^1.29",
        "phpstan/phpdoc-parser": "^1.15",
        "psr/cache": "^2.0|^3.0",
        "psr/clock": "^1.0",
        "psr/container": "^1.1|^2.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-message": "^1.0|^2.0",
        "psr/link": "^1.1|^2.0",
        "psr/log": "^1|^2|^3",
        "stof/doctrine-extensions-bundle": "^1.13.0",
        "symfony/asset": "7.1.*",
        "symfony/asset-mapper": "7.1.*",
        "symfony/console": "7.*",
        "symfony/contracts": "^v3.5",
        "symfony/debug-bundle": "7.*",
        "symfony/doctrine-messenger": "7.*",
        "symfony/dotenv": "7.*",
        "symfony/expression-language": "7.*",
        "symfony/flex": "^2",
        "symfony/form": "7.*",
        "symfony/framework-bundle": "7.*",
        "symfony/http-client": "7.*",
        "symfony/intl": "7.*",
        "symfony/mailer": "7.*",
        "symfony/mercure-bundle": "^0.3.7",
        "symfony/messenger": "7.*",
        "symfony/mime": "7.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.*",
        "symfony/polyfill-ctype": "~1.8",
        "symfony/polyfill-intl-grapheme": "~1.0",
        "symfony/polyfill-intl-icu": "~1.0",
        "symfony/polyfill-intl-idn": "^1.10",
        "symfony/polyfill-intl-normalizer": "~1.0",
        "symfony/polyfill-mbstring": "~1.0",
        "symfony/polyfill-php83": "^1.28",
        "symfony/polyfill-uuid": "^1.15",
        "symfony/process": "7.*",
        "symfony/property-access": "7.*",
        "symfony/property-info": "7.*",
        "symfony/requirements-checker": "^2.0",
        "symfony/runtime": "7.*",
        "symfony/security-bundle": "7.*",
        "symfony/serializer": "7.*",
        "symfony/stimulus-bundle": "*",
        "symfony/string": "7.*",
        "symfony/translation": "7.*",
        "symfony/twig-bundle": "7.*",
        "symfony/uid": "7.*",
        "symfony/ux-icons": "*",
        "symfony/ux-twig-component": "^2.25",
        "symfony/validator": "7.*",
        "symfony/web-link": "7.*",
        "symfony/yaml": "7.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.5",
        "twig/twig": "^3.12"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "App",
            "App\\StudentApp\\": "App/mod_student_app",
            "App\\MainApp\\": "App/MainApp"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MainApp\\Tests\\": "tests/MainApp",
            "StudentApp\\Tests\\": "tests/StudentApp"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "7.*"

    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.6",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.*",
        "symfony/css-selector": "7.*",
        "symfony/debug-bundle": "v7.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "7.*",
        "symfony/stopwatch": "7.*",
        "symfony/web-profiler-bundle": "7.*"
    }
}
